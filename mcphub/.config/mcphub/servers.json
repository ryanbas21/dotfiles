{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git", "--repository", "path/to/git/repo"]
    },
    "brave-search": {
      "args": ["-y", "@modelcontextprotocol/server-brave-search"],
      "command": "npx",
      "env": {
        "BRAVE_API_KEY": "$BRAVE_API_KEY"
      }
    },
    "chrome-devtools": {
      "args": ["chrome-devtools-mcp@latest"],
      "command": "npx"
    },
    "effect-mcp": {
      "args": ["-y", "effect-mcp@latest"],
      "command": "npx",
      "env": {}
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "~/programming",
        "/Users/ryan.basmajian/.config/nvim",
        "/Users/ryan.basmajian/.config",
        "/Users/ryan.basmajian/.config/nix"
      ],
      "command": "npx"
    },
    "memory": {
      "args": [
        "-y",
        "mcp-knowledge-graph",
        "--memory-path",
        "/Users/ryan.basmajian/.aim/"
      ],
      "command": "npx"
    },
    "nixos": {
      "args": ["run", "github:utensils/mcp-nixos", "--"],
      "command": "nix"
    },
    "nx-mcp": {
      "args": ["nx-mcp@latest"],
      "command": "npx"
    },
    "package-registry": {
      "args": ["-y", "package-registry-mcp"],
      "command": "npx"
    },
    "playwright": {
      "args": ["@playwright/mcp@latest"],
      "command": "npx"
    },
    "puppeteer": {
      "args": ["-y", "puppeteer-mcp-server"],
      "command": "npx",
      "env": {}
    },
    "serena": {
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ],
      "command": "/Users/ryan.basmajian/.local/bin/uvx"
    }
  }
}
