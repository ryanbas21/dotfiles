{
    "languageserver": {
        "haskell": {
            "command": "/Users/batman/.local/bin/hie-wrapper",
            "rootPatterns": [".stack.yaml", "cabal.config", "package.yaml"],
            "filetypes": ["hs", "lhs", "haskell"],
            "initializationOptions": {
                "languageServerHasell": {
                    "hlintOn": true
                }
            }
        },
        "reason": {
            "command": "/Users/batman/rls-macos/reason-language-server",
            "filetypes": ["reason", "re"]
        },
        "metals": {
            "command": "metals-vim",
            "rootPatterns": ["build.sbt"],
            "filetypes": ["scala", "sbt"]
        },
        "ocaml": {
            "command": "ocaml-language-server",
            "args": ["--stdio"],
            "filetypes": ["ocaml", "reason"]
        },
        "elmLS": {
            "command": "elm-language-server",
            "args": ["--stdio"],
            "filetypes": ["elm"],
            "rootPatterns": ["elm.json"],
            "initializationOptions": {
                "runtime": "node",
                "elmPath": "elm",
                "elmFormatPath": "elm-format",
                "elmTestPath": "elm-test"
            }
        }
    },
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.watchmanPath": "/usr/local/bin/watchman",
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.colorSupport": true,
    "coc.preferences.formatOnType": false,
    "coc.preferences.formatOnSaveFiletypes": ["js", "ts", "jsx", "less", "css"],
    "eslint.autoFixOnSave": false,
    "prettier.eslintIntegration": true,
    "prettier.bracketSpacing": true,
    "prettier.requireConfig": true,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.timeout": 500,
    "diagnostic.enable": false,
    "list.normalMappings": {
        "t": "action:tabe",
        "v": "action:vsplit",
        "s": "action:split",
        "d": "expr:GetDeleteAction"
    },
    "list.insertMappings": {
        "<C-t>": "action:tabe",
        "<C-v>": "action:vsplit",
        "<C-w>": "command:wincmd k"
    },
    "list.maxHeight": 100
}
